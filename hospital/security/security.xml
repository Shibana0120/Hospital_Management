<odoo>
  <data>
      <record model="ir.module.category" id="module_category_hospital">
        <field name="name">Hospital Management</field>
        <field name="description">Helps to Manage Hospital</field>
      </record>

      <record id="group_MIMS_Doctor" model="res.groups">
            <field name="name">Doctor</field>
            <field name="category_id" ref="module_category_hospital"/>
        </record>

      <record model="ir.rule" id="report_rule_patient">
        <field name="name">See only my patients</field>
        <field name="model_id" ref="model_hospital_patient"/>
        <field name="domain_force">[('doctor.user_id','=',user.name)]</field>
        <field name="groups" eval="[(4,ref('group_MIMS_Doctor'))]"/>
    </record>

      </data>
</odoo>