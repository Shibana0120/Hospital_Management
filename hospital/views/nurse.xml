<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="nurse_list">
      <field name="name">nurse list</field>
      <field name="model">hospital.nurse</field>
      <field name="arch" type="xml">
        <tree>
          <field name="first_name"/>
          <field name="last_name"/>
          <field name="DOB"/>
          <field name="image"/>
          <field name="gender"/>
          <field name="contact_number"/>
          <field name="email"/>
          <field name="address"/>
        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="nurse_form">
      <field name="name">nurse form</field>
      <field name="model">hospital.nurse</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
          <field name="first_name"/>
          <field name="last_name"/>
          <field name="DOB"/>
          <field name="image"/>
          <field name="gender"/>
          <field name="contact_number"/>
          <field name="email"/>
          <field name="address"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record model="ir.actions.act_window" id="nurse_action_window">
      <field name="name">nurse window</field>
      <field name="res_model">hospital.nurse</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="MIMS Hospital" id="hospital_menu_root"/>

    <menuitem name="Nurse" id="nurse_menu" parent="hospital_menu_root"
              action="nurse_action_window"/>

  </data>
</odoo>